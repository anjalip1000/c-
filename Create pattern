// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
    int a;
    for(a=10;a>=6;a--)
    {
        std::cout <<a;
        
    }
    std::cout << "\n";
    for(a=5;a>=1;a--)
    {
        std::cout<<a;
    }
    std::cout<< "\n";
    for(a=4;a>=1;a--)
    {
        std::cout<<a;
    }
     std::cout<< "\n";
    for(a=3;a>=1;a--)
    {
        std::cout<<a;
    }
    std::cout<< "\n";
    for(a=2;a>=1;a--)
    {
        std::cout<<a;
    }
   std::cout<< "\n";
   std::cout <<a;
    return 0;
}

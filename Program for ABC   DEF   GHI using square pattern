#include<bits/stdc++.h>
using namespace std;
using namespace std;
int main()
{ int i=1,n;
cin >> n;
char start ='A';
while(i<=n){
       int j=1;
       while(j<=n){
              cout << start;
              start=start+1;
              j=j+1;
       }
       cout <<"\n";
       i=i+1;
}
       return 0; }

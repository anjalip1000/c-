#include<bits/stdc++.h>
using namespace std;
using namespace std;
int main()
{ int i=1,n;
cin >> n;
while(i<=n){
       int j=1;
       char ch='A'+n-i;
       while(j<=i){
         
         cout << ch <<" ";
         ch=ch+1;
           j=j+1;   
       }
       cout <<"\n";
       i=i+1;
}
       return 0; }

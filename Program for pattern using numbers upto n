// Online C++ compiler to run C++ program online
#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main() {
    int i,j,n;
    cout << "how many lines you want----> " << "\n";
    cin >> n;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
           
                 cout << j;
            
             }
    
        cout << "\n" <<"";
    }
    return 0;
}

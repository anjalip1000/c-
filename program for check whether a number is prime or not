#include<bits/stdc++.h>
using namespace std;
int main()
{ int n,i=2;
    cin >> n;
    while(i<n){
           if(n%i==0){
           cout << "it is not prime" ;
                  break ;
           }
           else{
                 cout<< "Its prime" ;
                  break ;
           }
              
              i=i+1; }
    return 0; }

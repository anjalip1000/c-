#include<bits/stdc++.h>
using namespace std;
int main()
{
       int i,j;
        for(i=0;i<2;i++){
              
              for(j=0;j<2;j++){
                     char ch =('A'+i);
                     cout<< ch;
                   
              }
              cout <<"\n";
       }
         return 0; 
}
